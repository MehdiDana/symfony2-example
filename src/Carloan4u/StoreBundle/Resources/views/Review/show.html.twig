<h1>Review</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Dealership</th>
            <td>{{ entity.dealership.name }}</td>
        </tr>
        <tr>
            <th>Rate</th>
            <td>{{ entity.rate }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Review_title</th>
            <td>{{ entity.reviewtitle }}</td>
        </tr>
        <tr>
            <th>Review_description</th>
            <td>{{ entity.reviewdescription }}</td>
        </tr>
        <tr>
            <th>Approved</th>
            <td>{{ entity.approved }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('review') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('review_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('review_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
